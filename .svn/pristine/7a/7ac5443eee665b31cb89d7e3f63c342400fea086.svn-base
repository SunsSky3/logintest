    background: #79cd40;
  }
  #divInfo #detailedInfo:hover{
    background: #8dd45e;
  }
  #divInfo #cancelReservBtn{
    background: #ed1941;
  }
  #divInfo #cancelReservBtn:hover{
    background: #ef4136;
  }
  /*                 信息弹窗 end               */     

  /*                   背景变红类                */
  .backgroundRed{
    background-color: red;
  }
   /*                      背景变红类end             */
</style>

